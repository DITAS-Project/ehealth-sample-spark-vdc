# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /webjars/*file                    controllers.WebJarAssets.at(file)

# GET     /locations/json       controllers.ApplicationController.index


GET	/get-patient-details/:socialId	controllers.EHealthVDCController.getPatientDetails(socialId: String)
GET     /get-test-values/:testType/:patientId      controllers.EHealthVDCController.getTestValues(patientId: String, testType: String)
